.container {
  width: 100%;
  display: flex;
  flex-direction: column;
  font-size: 17px;
}

.navigator {
  width: 100%;
  height: 92px;
  position: fixed;
  top: 0rpx;
  z-index: 1;
}

.block-title {
  width: 100%;
  height: 50px;
  background-color: #f2f8fd;
}

.title {
  height: 42px;
  width: 100%;
  display: flex;
  flex-direction: row;
  padding: 0 5%;
  font-size: 40rpx;
  background-color: #f2f8fd;
}

.title image {
  height: 55rpx;
  width: 55rpx;
  padding: 2%;
}

.title text {
  padding: 1% 25%;
}

.judge {
  margin-top: 92px;
}

.acc-icon {
  width: 90%;
  height: 180rpx;
  padding: 2% 5%;
  display: flex;
  flex-direction: row;
}

.icon-text {
  width: 20%;
  margin: 0;
}

.acc-images {
  width: 80%;
}

.images-form{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}

.images-form-1,
.images-form-2 {
  display: flex;
  flex-direction: row;
}

.images-part-1{
  margin: 0 15% 4% 0;
  width: 75rpx;
  height: 75rpx;
}

.images-part-2{
  margin: 0 15% 4% 0;
  width: 75rpx;
  height: 75rpx;
}

.images-part-1 image,
.images-part-2 image {
  width: 75rpx;
  height: 75rpx;
}

.acc-name,
.acc-money,
.acc-payway {
  background-color: #f2f8fd;
  width: 90%;
  height: 90rpx;
  padding: 2% 5%;
  display: flex;
  flex-direction: row;
  line-height: 95rpx;
}

.name-l,
.data-l,
.money-l,
.remarks-l {
  width: 20%;
}

.payway-l {
  width: 30%;
}

.type-r,
.name-r,
.data-r,
.money-r,
.remarks-r {
  width: 80%;
  height: 90rpx;
  line-height: 95rpx;
  color: grey;
}

.payway-r {
  width: 70%;
  height: 90rpx;
  line-height: 95rpx;
  color: grey;
}

.acc-date,
.acc-remarks
 {
  width: 90%;
  height: 90rpx;
  padding: 2% 5%;
  display: flex;
  flex-direction: row;
  line-height: 95rpx;
}

.submit {
  width: 90%;
  height: 90rpx;
  margin-top: 5%;
  background-color: #f2f8fd;
  border-radius: 25rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}

.submit::after {
  border-radius: 25rpx;
  border: 0;
}